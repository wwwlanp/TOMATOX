{
  "name": "tomatox",
  "version": "2.0.3",
  "description": "A free fast beautiful online video player with electron.",
  "main": "./dist/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.main.prod.config.js",
    "build-renderer": "cross-env NODE_ENV=production webpack --config webpack.renderer.prod.config.js",
    "build": "npm run build-main && npm run build-renderer",
    "start-renderer-dev": "webpack-dev-server --config webpack.renderer.dev.config.js",
    "start-main-dev": "webpack --config webpack.main.config.js && electron ./dist/main.js",
    "start-dev": "cross-env START_HOT=1 npm run start-renderer-dev",
    "prestart": "npm run build",
    "start": "electron .",
    "lint": "eslint --ext=jsx,js,tsx,ts src --fix",
    "test": "jest '(\\/test\\/(?!e2e/)).*'",
    "pretest:e2e": "npm run build",
    "test:e2e": "jest '(\\/test\\/e2e/).*'",
    "unpacked": "npm run build && electron-builder --dir",
    "pack": "npm run build && electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "release": "npm run build && electron-builder -p always"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src,test,mocks}/**/*.{json,css,scss,md}": [
      "prettier --config ./.prettierrc --write"
    ],
    "{src,test,mocks}/**/*.{js,ts,tsx}": [
      "prettier --config ./.prettierrc --write",
      "eslint --ext=jsx,js,ts,tsx --fix src"
    ]
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/test/.+\\.spec)\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json",
      "node"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/mocks/fileMock.js",
      "\\.(s?css|sass)$": "<rootDir>/mocks/styleMock.js"
    }
  },
  "build": {
    "productName": "TOMATOX",
    "copyright": "Copyright @ 2021 yanjiaxuan",
    "appId": "github.yanjiaxuan.tomatox",
    "publish": [
      {
        "provider": "github",
        "owner": "yanjiaxuan",
        "repo": "TOMATOX"
      }
    ],
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/",
      "package.json"
    ],
    "win": {
      "icon": "public/icon256.ico",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "linux": {
      "icon": "public/icon.png"
    },
    "mac": {
      "icon": "public/icon.icn",
      "category": "public.app-category.developer-tools",
      "target": "default",
      "extendInfo": {
        "LSUIElement": 1
      }
    },
    "snap": {
      "publish": [
        "github"
      ]
    },
    "nsis": {
      "installerIcon": "public/icon256.ico",
      "installerHeaderIcon": "public/icon256.ico",
      "uninstallerIcon": "public/icon256.ico",
      "uninstallDisplayName": "TOMATOX",
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true
    }
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:yanjiaxuan/TOMATOX.git"
  },
  "files": [
    "dist/",
    "package.json"
  ],
  "author": {
    "name": "yanjiaxuan",
    "email": "330544968@qq.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yanjiaxuan/TOMATOX/issues"
  },
  "homepage": "https://github.com/yanjiaxuan/TOMATOX",
  "devDependencies": {
    "@babel/core": "7.12.3",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/polyfill": "^7.4.4",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.14.0",
    "@hot-loader/react-dom": "^16.8.6",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/electron-localshortcut": "^3.1.0",
    "@types/jest": "^24.0.13",
    "@types/react": "^17.0.5",
    "@types/react-dom": "^17.0.5",
    "@types/react-infinite-scroller": "^1.2.1",
    "@types/react-redux": "^7.0.9",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-test-renderer": "^16.8.1",
    "@types/webdriverio": "^4.8.7",
    "@types/webpack-env": "^1.13.3",
    "@typescript-eslint/eslint-plugin": "^2.4.0",
    "@typescript-eslint/parser": "^2.4.0",
    "antd": "^4.15.5",
    "axios": "^0.21.1",
    "babel-loader": "^8.1.0",
    "cheerio": "^1.0.0-rc.9",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^5.1.3",
    "css-loader": "^2.1.1",
    "electron": "^3.1.9",
    "electron-builder": "^22.3.2",
    "electron-devtools-installer": "^2.2.4",
    "electron-localshortcut": "^3.2.1",
    "electron-proxy-agent": "^1.2.0",
    "electron-updater": "^4.3.9",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^1.7.0",
    "fast-xml-parser": "^3.19.0",
    "file-loader": "^3.0.1",
    "fork-ts-checker-webpack-plugin": "^1.3.4",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^4.2.1",
    "interpolate-html-plugin": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^10.0.7",
    "m3u8-parser": "^4.7.0",
    "mini-css-extract-plugin": "^1.6.0",
    "prettier": "^1.18.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hot-loader": "^4.8.8",
    "react-infinite-scroller": "^1.2.4",
    "react-keeper": "^2.2.3",
    "react-redux": "^7.0.3",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^16.8.6",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.5",
    "sass": "^1.33.0",
    "sass-loader": "^7.1.0",
    "socks-proxy-agent": "^5.0.0",
    "source-map-loader": "^0.2.4",
    "spectron": "^5.0.0",
    "style-loader": "^0.23.1",
    "svg-url-loader": "^7.1.1",
    "ts-jest": "^24.0.2",
    "typescript": "^4.2.4",
    "url-loader": "^4.1.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1",
    "webpack-merge": "^4.2.1",
    "xgplayer": "^2.20.8",
    "xgplayer-flv.js": "^2.3.0",
    "xgplayer-hls.js": "^2.4.2"
  },
  "dependencies": {}
}
